includes:
	- phpstan-baseline.neon

parameters:
	checkMissingIterableValueType: false

	ergebnis:
		classesAllowedToBeExtended:
			- Gansel\PhpCsFixer\Config\RuleSet\AbstractRuleSet
			- Gansel\PhpCsFixer\Config\Test\Unit\RuleSet\AbstractRuleSetTestCase

	inferPrivatePropertyTypeFromConstructor: true

	level: max

	paths:
		- src/
		- test/

	tmpDir: .build/phpstan/
