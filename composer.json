{
  "name": "gansel-rechtsanwaelte/php-cs-fixer-config",
  "type": "library",
  "description": "Provides a configuration factory and multiple rule sets for friendsofphp/php-cs-fixer.",
  "homepage": "https://github.com/gansel-rechtsanwaelte/php-cs-fixer-config",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "friendsofphp/php-cs-fixer": "~2.19.2"
  },
  "require-dev": {
    "composer/package-versions-deprecated": "1.11.99.3",
    "ergebnis/composer-normalize": "^2.15.0",
    "ergebnis/license": "^1.1.0",
    "ergebnis/phpstan-rules": "~0.15.3",
    "ergebnis/test-util": "^1.5.0",
    "infection/infection": "~0.15.3",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "~0.12.96",
    "phpstan/phpstan-deprecation-rules": "~0.12.6",
    "phpstan/phpstan-phpunit": "~0.12.22",
    "phpstan/phpstan-strict-rules": "~0.12.11",
    "phpunit/phpunit": "^8.5.20"
  },
  "config": {
    "platform": {
      "php": "7.2.33"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  },
  "autoload": {
    "psr-4": {
      "Gansel\\PhpCsFixer\\Config\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gansel\\PhpCsFixer\\Config\\Test\\": "test"
    }
  },
  "support": {
    "issues": "https://github.com/gansel-rechtsanwaelte/php-cs-fixer-config/issues",
    "source": "https://github.com/gansel-rechtsanwaelte/php-cs-fixer-config"
  }
}
